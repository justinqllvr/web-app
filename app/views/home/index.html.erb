<h1>Ma ville accessible</h1>

<div id="map"></div>
<script>
  var map;
  function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: 44.841637, lng: -0.569082},
      zoom: 12
    });
  }
  function placereport(lat,lng) {
  		var myLatLng = {lat: lat , lng: lng};

  		var marker = new google.maps.Marker({
    		position: myLatLng,
    		map: map,
    		title: 'point'
  		});
	}

  <% @reports.each do |report| %>
  	placereport(<%= report.latitude %>,<%= report.longitude %>)
  <% end %>

</script>


<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCZBpHXZJ47s2jgLcHA1kqSVY_2ya2Nuak&callback=initMap"
    async defer></script>

